      <script>
        
        $( document ).ready(function() {
            roi_onchange();
            stack_roi_click(3);
        });
        

        function roi_onchange()
        {
            var roi_amount = $("#roi_amount").val().trim();
            var roi_period = $("#roi_period_range").val().trim();
            $("#roi_period").text(roi_period + " month");
            if(parseInt(roi_period) < 3)
            {
                $("#roi_rate").text($("#hdd_roi1").val());
                $("#estim_progress").text(40);
            }
            else if(parseInt(roi_period) < 6)
            {
                $("#roi_rate").text($("#hdd_roi3").val());
                $("#estim_progress").text(45);
            }
            else if(parseInt(roi_period) < 9)
            {
                $("#roi_rate").text($("#hdd_roi6").val());
                $("#estim_progress").text(50);
            }
            else
            {
                $("#roi_rate").text($("#hdd_roi12").val());
                $("#estim_progress").text(55); 
            }
            $("#span_month").text(parseInt(roi_period));
            var roi_rinterest = $("#roi_rate").text(); 
            const periodInYears = parseInt(roi_period) / 12;
            const totalInterest = parseInt(roi_amount) * ((roi_rinterest) / 100) * periodInYears;
            var totalreturn =    parseInt(roi_amount) + totalInterest;
            var monthlyreturn = totalInterest / parseInt(roi_period);
            $("#span_month_interest").text(parseFloat(monthlyreturn).toFixed(4) + " Piπ");
            $("#span_total_interest").text(parseFloat(totalreturn).toFixed(4) + " Piπ");
        }

        function stack_roi_click(check)
        {
            var stack_amt = $("#stack_amount").val().trim();
            var stack_apr = '';
            if(check == 0)
            {
                check = $("#stack_buttons").find(".border-bitcoin-orange").attr("data-month");
            }     
            $("#stack_buttons").find("button").removeClass("border-bitcoin-orange");
            $("#stack_buttons").find("button").removeClass("grid_select");
            $("#stack_buttons").find("button").addClass("grid_select");
            $("#btn_stack_roi"+check).addClass("border-bitcoin-orange");
            $(".stack_total_month").text(check);
            if(parseInt(check) < 3)
            {
                stack_apr = $("#hdd_roi1").val();
            }
            else if(parseInt(check) < 6)
            {
                stack_apr = $("#hdd_roi3").val();
            }
            else if(parseInt(check) < 9)
            {
                stack_apr = $("#hdd_roi6").val();
            }
            else
            {
                stack_apr = $("#hdd_roi12").val();
            }
            const annualReturn = stack_amt * (stack_apr / 100);
            const monthlyReturn =  annualReturn / 12;
            $(".monthly_stack_val").text(parseFloat(monthlyReturn).toFixed(4));
            $(".total_stack_val").text(parseFloat(  monthlyReturn * parseInt(check) ).toFixed(4));
        }
      </script>
