            <p class="text-xl font-bold font-mono">`+ parseFloat(act_amt).toFixed(4)+` Piπ</p>
            <p class="stake_small text-sm text-muted-foreground">`+ stack_from_date+` - `+ stack_to_date+`</p>
         </div>
         <div class="bg-bitcoin-orange/10 text-bitcoin-orange px-3 py-1 rounded-full text-sm font-medium">`+apr_val+`% APR</div>
      </div>
      <div class="space-y-3">
         <div class="flex items-center justify-between text-sm">
            <span class="data_text text-muted-foreground flex items-center">
               <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="class_clock lucide lucide-clock mr-1 h-3 w-3" >
                  <circle cx="12" cy="12" r="10"></circle>
                  <polyline points="12 6 12 12 16 14"></polyline>
               </svg>
               Progress
            </span>
            <span class="class_perce font-medium">`+remaining_days+` days</span>
         </div>
         <progress min="0" max="100" value="`+complete_days+`" class="stack_progress" style="width:100%;margin-top: 5%;" ></progress>
         <div class="h-2 bg-secondary rounded-full overflow-hidden">
            <div class="h-full bg-gradient-to-r from-bitcoin-orange to-bitcoin-light transition-all duration-1000" style="width: 100%;"></div>
         </div>
      </div>
   </div>
</div>`
            });
            $('#all_stake_data').html(html); 
        }

        function getstakedetails()
        {
            $.ajax({
                type: "POST",
                url: "/api/webapi/stake/details",
               data: {
                authtoken:auth_token,     
              },
                dataType: "json",
                success: function (response) {
                    $(".total_stake").text(parseFloat(response.totalstakes).toFixed(4));
                    $(".total_users").text(response.totalstakeusers);
                    $(".rewards_paid").text(parseFloat(response.totalclaimedrewards).toFixed(4));
                    getactivestakes(response.useractivestakes);
                    getallstakes(response.userstakedetails);
                }
              });
        }

        function stack_roi_click(check)
        {
            var stack_amt = $("#stack_amount").val().trim();
            var stack_apr = '';
            if(check == 0)
            {
                check = $("#stack_buttons").find(".border-bitcoin-orange").attr("data-month");
            }     
            $("#stack_buttons").find("button").removeClass("border-bitcoin-orange");
            $("#stack_buttons").find("button").removeClass("grid_select");
            $("#stack_buttons").find("button").addClass("grid_select");
            $("#btn_stack_roi"+check).addClass("border-bitcoin-orange");
            $(".stack_total_month").text(check);
            if(parseInt(check) < 3)
            {
                stack_apr = $("#hdd_roi1").val();
            }
            else if(parseInt(check) < 6)
            {
                stack_apr = $("#hdd_roi3").val();
            }
            else if(parseInt(check) < 9)
            {
                stack_apr = $("#hdd_roi6").val();
            }
            else
            {
                stack_apr = $("#hdd_roi12").val();
            }
            const annualReturn = stack_amt * (stack_apr / 100);
            const monthlyReturn =  annualReturn / 12;
            $(".monthly_stack_val").text(parseFloat(monthlyReturn).toFixed(4));
            $(".total_stack_val").text(parseFloat(  monthlyReturn * parseInt(check) ).toFixed(4));
        }
    });
}
catch (err) {
  alert(err);

}
}
auth();
      </script>
