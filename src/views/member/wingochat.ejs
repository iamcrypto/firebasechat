    <script type="module">
        import { initializeApp } from "https://www.gstatic.com/firebasejs/9.15.0/firebase-app.js";
        import { getDatabase, ref, set, remove, onChildAdded, onChildRemoved } from "https://www.gstatic.com/firebasejs/9.15.0/firebase-database.js";
  
        var seconds = 1000 * 60;
        var timer;
        
        function calltimer()
        {
          var timer2 = "0:60";
        var interval = setInterval(function() {
        
        
          var timer = timer2.split(':');
          //by parsing integer, I avoid all extra string processing
          var minutes = parseInt(timer[0], 10);
          var seconds = parseInt(timer[1], 10);
          --seconds;
          minutes = (seconds < 0) ? --minutes : minutes;
          seconds = (seconds < 0) ? 59 : seconds;
          seconds = (seconds < 10) ? '0' + seconds : seconds;
          //minutes = (minutes < 10) ?  minutes : minutes;
          $('#timer').html(minutes + ':' + seconds);
          if (minutes < 0) clearInterval(interval);
          //check if both minutes and seconds are 0
          if ((seconds <= 0) && (minutes <= 0)) clearInterval(interval);
          timer2 = minutes + ':' + seconds;
        }, 1000);
        
        }

        var interval_in;
        function cownDownTimer() {
          var countDownDate = new Date("2030-07-16T23:59:59.9999999+01:00").getTime();
          interval_in = setInterval(function () {
              var now = new Date().getTime();
              var distance = countDownDate - now;
              var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
              var minute = Math.ceil(minutes % Number(1));
              var seconds1 = Math.floor((distance % (1000 * 60)) / 10000);
              var seconds2 = Math.floor(((distance % (1000 * 60)) / 1000) % 10);
              const p_details = `<%=d_period%>`;
              const a_details = `<%=d_amount%>`;
              $('#timer').html('0' + ':' + seconds1 + seconds2);
              $("#period_id").text(p_details);
              $(".b_number").text(a_details);
              if( parseInt(a_details) == 5 || parseInt(a_details)== 0)
              {
                  $(".num_color").css('background-color', '#eb43dd');
              }
              else if (parseInt(a_details) % 2 == 0) {
                  $(".num_color").css('background-color', '#fb4e4e');
              }
              else{
                  $(".num_color").css('background-color', '#5cba47');
              }
          }, 1000);
      }
      cownDownTimer();
      const Pi = window.Pi;
Pi.init({ version: "2.0", sandbox: <%=sandbox%> });
async function auth() {
  try {
      
      const scopes = ['username', 'payments', 'wallet_address'];
      function onIncompletePaymentFound(payment) {
          console.log("incomplete Transaction");
      }; 

      Pi.authenticate(scopes, onIncompletePaymentFound).then(function(auth) {
          var username = auth.user.username;
          var password = auth.user.uid;
          var auth_token = auth.accessToken;
          const socket = io();
          
  
          $('.name').text(username);
          var chatname = username;
        socket.on("data-server-chat", function (msg) {
            if (msg) {
              clearInterval(interval_in);
              calltimer();
                $("#period_id").text(msg.data[1].period);
                $(".b_number").text(msg.data[1].amount);
                if(parseInt(msg.data[1].amount) == 5 || parseInt(msg.data[1].amount) == 0)
                {
                    $(".num_color").css('background-color', '#eb43dd');
                }
                else if (parseInt(msg.data[1].amount) % 2 == 0) {
                    $(".num_color").css('background-color', '#fb4e4e');
                }
                else{
                    $(".num_color").css('background-color', '#5cba47');
                }
            }
        });
      const firebaseConfig = {
        apiKey:  "<%=d_f_api%>",
        authDomain:  "<%=d_f_authdomain%>",
        databaseURL: "<%=d_f_dburl%>",
        projectId:  "<%=d_f_projid%>",
        storageBucket:  "<%=d_f_stobck%>",
        messagingSenderId:  "<%=d_f_messId%>",
        appId:  "<%=d_f_appid%>",
        measurementId:  "<%=d_f_mesuareId%>"
        };
      
        const app = initializeApp(firebaseConfig);
        const db = getDatabase(app);
        var msgTxt = document.getElementById('msgTxt');
        var sender = chatname;

        module.sendMsg = function sendMsg(){
            var msg = msgTxt.value;
            var date = new Date();
            var timestamp = new Date().getTime();
            set(ref(db,"messages/"+timestamp),{
                msg : msg,
                sender : sender,
                status:"active",
                datetime:  moment().format('DD/MM/YYYY'),
                time:moment().format("h:mm a"),
            })
            msgTxt.value=""; 
        }
        onChildAdded(ref(db,"messages"), (data)=>{
          if(data.val().sender == sender){
              messages.innerHTML += "<div style=justify-content:end class=outer id="+data.key+"><div id=inner class=me>you : "+data.val().msg+ " "+data.val().time+ "<button id=dltMsg onclick=module.dltMsg("+data.key+")><i class='fa fa-trash-o'></i></button></div></div>";
          } else {
              messages.innerHTML += "<div class=outer id="+data.key+"><div id=inner class=notMe>"+data.val().sender+" : "+data.val().msg+ " "+data.val().time+ "</div></div>";
          }
        $('#messages').scrollTop($('#messages')[0].scrollHeight);
      })
      module.dltMsg = function dltMsg(key){
        remove(ref(db,"messages/"+key));
    }
    onChildRemoved(ref(db,"messages"),(data)=>{
        var msgBox = document.getElementById(data.key);
        messages.removeChild(msgBox);
    })
        
		  });
    }
catch (err) {
    alert(err);
}
}
auth();
          
    </script>
